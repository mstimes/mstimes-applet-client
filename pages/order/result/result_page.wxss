@import '../../../style/css/icons.wxss';

.horizon_line {
  width:100%;
  height:1rpx;
  background:rgb(236, 235, 235);
}

.row-space-between {
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 26rpx;
  font-weight: 400;
  margin-left: 50rpx;
  margin-right: 30rpx;
  margin-top: 30rpx;
}

.return-home-container{
  font-size: 40rpx;
  display: flex;
  width: 100%;
  justify-content: center;
  padding: 30px 0px 5px 0px;
}

.return-home-butt {
  font-size: 24rpx; 
  background-color:white;
  border-radius: 1px;
  border-width: 1px;
  background:white;
  border: 1px solid rgb(216, 214, 214);
  align-items: center;
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;  
}